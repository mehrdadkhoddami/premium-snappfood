{
  "manifest_version": 3,
  "name": "Premium SnappFood",
  "version": "1.0",
  "description": "Hide vendor cars, product cards and banners which labeled as Ads on snappfood.ir.",
  "content_scripts": [
    {
      "matches": ["*://*.snappfood.ir/*", "*://snappfood.ir/*"],
      "js": ["contentScript.js"],
      "css": ["styles.css"],
      "run_at": "document_idle"
    }
  ],
  "host_permissions": ["*://*.snappfood.ir/*"],
  "icons": {
    "16": "icon-16.png",
    "48": "icon-48.png",
    "128": "icon-128.png"
  }
}